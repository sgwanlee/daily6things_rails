$('span#task-priority-<%= @task.id %>').text('<%= @task.priority %>');
$('#edit_task_<%= @task.id %>').data("priority", <%= @task.priority %>);

$("#incompleted-tasks>form").each(function(){
  var prev_priority = parseInt($(this).data("priority"))
  var current_priority = <%= @task.priority %>;

  if (prev_priority > current_priority) {
    $('#edit_task_<%= @task.id %>').insertAfter($(this));
  }
});