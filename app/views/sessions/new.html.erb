<%= form_for(:session, url: login_path, html: {class: "login-form center col-md-4 col-xs-12 col-sm-10"}) do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email, autocomplete: false%>

  <%= f.label :password %>
  <%= f.password_field :password, autocomplete: false%>

  <%= f.submit "Log in" %>
<% end %>